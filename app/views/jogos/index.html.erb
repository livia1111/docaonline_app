<h1>Listing jogos</h1>

<table>
  <tr>
    <th>Tipo de midia</th>
    <th>Requisitos computacionais</th>
    <th>Genero</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @jogos.each do |jogo| %>
  <tr>
    <td><%= jogo.tipo_de_midia %></td>
    <td><%= jogo.requisitos_computacionais %></td>
    <td><%= jogo.genero %></td>
    <td><%= link_to 'Show', jogo %></td>
    <td><%= link_to 'Edit', edit_jogo_path(jogo) %></td>
    <td><%= link_to 'Destroy', jogo, confirm: 'Are you sure?', method: :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Jogo', new_jogo_path %>
